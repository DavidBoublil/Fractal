<UserControl x:Class="Fractal.Views.ShapeControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Fractal"
             xmlns:viewModels="clr-namespace:Fractal.ViewModels"
             mc:Ignorable="d" 
             d:DataContext="{d:DesignInstance viewModels:ShapeControlViewModel}"
             d:DesignHeight="450" d:DesignWidth="800">

    <Border CornerRadius="{Binding CornerRadius}" Background="{Binding BackgroundColor}" BorderThickness="1" BorderBrush="Gray">
        <Grid>

        </Grid>
    </Border>

</UserControl>
